{
    "env": {
      "browser": true,
      "es2021": true
    },
    "extends": [
      "plugin:@typescript-eslint/strict-type-checked",
      "prettier",
      "plugin:prettier/recommended"
    ],
    "overrides": [
      {
        "files": ["*.js"],
        "parser": "espree"
      }
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "ecmaVersion": 12,
      "sourceType": "module",
      "project": ["./tsconfig.json"]
    }
  }